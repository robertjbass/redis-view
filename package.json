{
  "name": "redis-view",
  "version": "0.1.6",
  "description": "Visual Redis testing tool - test your local Redis installation",
  "type": "module",
  "bin": {
    "redis-view": "bin/redis-view.js"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "start": "tsx bin/cli.ts",
    "dev": "tsx watch bin/cli.ts",
    "format": "prettier --write .",
    "lint": "eslint ."
